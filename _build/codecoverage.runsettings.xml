<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
    <DataCollectionRunSettings>
        <DataCollectors>
            <DataCollector friendlyName="Code Coverage" uri="datacollector://Microsoft/CodeCoverage/2.0" assemblyQualifiedName="Microsoft.VisualStudio.Coverage.DynamicCoverageDataCollector, Microsoft.VisualStudio.TraceCollector, Culture=neutral">
                <Configuration>
                    <CodeCoverage>                        <!-- Match assembly file paths: -->
                        <ModulePaths>
                        <Include>
                              <ModulePath>.*Singularity.*</ModulePath>
                        </Include>
                            <Exclude>
                                <ModulePath>.*benchmark.dll</ModulePath>
                                <ModulePath>.*test.dll</ModulePath>
                                <ModulePath>.*testclasses.dll</ModulePath>
                            </Exclude>
                        </ModulePaths>
                        <Functions>
                            <Exclude>
                                <!-- Exclude methods in a class or namespace named UnitTest: -->
                                <Function>.*\.FastExpressionCompiler\..*</Function>
                            </Exclude>
                        </Functions>
                    </CodeCoverage>
                </Configuration>
            </DataCollector>
        </DataCollectors>
    </DataCollectionRunSettings>
</RunSettings>